<mvc:View
	controllerName="com.test.e2e.view.Detail"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<Page
		showNavButton="{device>/isPhone}"
		navButtonPress="onNavBack"
		class="sapUiFioriObjectPage"
		title="{i18n>detailTitle}">
		<content>
			<ObjectHeader
				 
					title="{Name}"
				
				 
					number="{Price}"
				
				 
					numberUnit="{CurrencyCode}"
				>
				 
					<ObjectAttribute text="{ShortDescription}"/>
				
				 
				<ObjectAttribute text="{LongDescription}"/>
				
			</ObjectHeader>
			<IconTabBar
				select="onDetailSelect"
				id="idIconTabBar">
				<items>
					<IconTabFilter
						key="Supplier"
						text="Supplier"
						icon="sap-icon://hint">
						<!--key="Detail" -->
						<content>
							<core:Fragment fragmentName="com.test.e2e.view.DetailInfoForm" type="XML" />
						</content>
					</IconTabFilter>
				</items>
			</IconTabBar>
		</content>
		<footer>
			<Toolbar>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>